<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="/style/style.css">
    <title>Description</title>
</head>

<body>
    <% if (typeof user != 'undefined') { %>
        <%- include("./partials/userNav.ejs") %> 
      <% } else {%>
        <%- include("./partials/guestNav.ejs") %> 
      <% } %> 
      
    <form>
        <input class="previous round db" type="button" value="&#8249;" onclick="history.back()">

    </form>
    <!-- <a href="#" class="previous round db">&#8249;</a> -->
    <div class="main-description-container">
        <div class="workout-header">
            <h2 class="plans-title description-title">
                <%=workout[0].name %>
            </h2>
        </div>

        <div class="description-container">
            <p>
            <pre class="description-text"> <%=workout[0].description %></pre>
            </p>
        </div>
        <div class="video-frame">
            <iframe width="700" height="450" src="<%=workout[0].url %>" title="YouTube video player" frameborder="0"
                allow="accelerometer; autoplay; clipboard-write; encrypted-media; gyroscope; picture-in-picture"
                allowfullscreen></iframe>
        </div>

    </div>


    <script src="/js/navbar.js"></script>
    
</body>

</html>